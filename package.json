{
  "name": "wee-core",
  "version": "2.0.0-alpha.6",
  "description": "Core Wee library and build scripts",
  "homepage": "https://www.weepower.com",
  "license": "Apache-2.0",
  "keywords": [
    "css",
    "framework",
    "javascript"
  ],
  "repository": "weepower/wee-core",
  "bugs": "https://github.com/weepower/wee/issues",
  "scripts": {
    "dist": "webpack --config=webpack.config.js",
    "test:scripts": "karma start ./tests/scripts/config/karma.dev.conf.js",
    "test:styles": "mocha ./tests/styles/**/*.js",
    "test": "npm run test:scripts",
    "test:ci-ie": "NODE_ENV='ci' karma start ./tests/scripts/config/karma.browserstack.ie.conf.js",
    "test:ci": "NODE_ENV='ci' karma start ./tests/scripts/config/karma.browserstack.conf.js"
  },
  "dependencies": {
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "extract-comments": "^0.7.3",
    "fs-extra": "^2.1.2",
    "fuzzy": "^0.1.3",
    "glob": "^7.1.1",
    "node-notifier": "^5.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "7.0.0-beta.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.24.0",
    "chai": "^3.5.0",
    "codecov": "^2.1.0",
    "common-tags": "^1.4.0",
    "karma": "^1.5.0",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "postcss": "^5.2.16",
    "postcss-js-mixins": "^2.5.0",
    "postcss-variables": "^1.1.0",
    "postcss-wee-syntax": "^2.0.1",
    "webpack": "^2.2.1"
  }
}
